@using System.Linq.Expressions;
@typeparam TEntity;
@typeparam TProp;
@inherits FieldBase<TEntity, TProp>;

<div class="mb-3 form-check form-switch">
    <label for="@PropertyName" class="form-label">@DisplayName</label>
    <input type="checkbox" class="form-check-input" role="switch" id="@PropertyName" checked="@PropertyValue" disabled>
</div>
